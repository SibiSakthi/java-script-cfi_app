const count =false; //true;//to initialise the conditions

let countValue = new Promise(function (resolve, reject) {//initilise promise
    if (count) {
        resolve("There is a count value.");
    } else {
        reject("There is no count value");
    }
});

countValue.then(function ifscucess(){alert("sucess");})//if sucess
countValue.catch(function iffailuier(){alert("failuier");})//if failure
